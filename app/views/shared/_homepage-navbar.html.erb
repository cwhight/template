<div class="fixed-top d-flex justify-content-between align-items-center mobile-hidden" id="homepage-navbar">
   <%= link_to image_tag("https://res.cloudinary.com/cwhight/image/upload/v1587654447/template_t_white_qmkzcy.png"), current_user&.employer ? dashboard_employer_path : root_path, class: "p-3 t-logo", id: "white-t"  %>
   <%= link_to image_tag("https://res.cloudinary.com/cwhight/image/upload/v1587655300/template_t_blue_vuz04r.png"), current_user&.employer ? dashboard_employer_path : root_path, class: "d-none p-3 t-logo", id: "blue-t"  %>
  <% if current_user %>
    <div class="d-flex flex-column justify-content-around px-2">
      <%= render 'shared/avatar', class: 'homepage-avatar ' %>
    </div>
  <% else %>
    <div class="d-flex">
      <div class="">
        <%= link_to 'Sign in', new_user_session_path, class: "btn btn-login" %>
      </div>
      <div class="px-2">
        <%= link_to 'Sign up', new_user_registration_path, class: "btn btn-sign-up" %>
      </div>
    </div>
  <% end %>

</div>

<div class="modal fade" id="modalContactForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
  aria-hidden="true">
  <div class="modal-dialog signup-modal-master" role="document">
    <div class="modal-content signup-modal">
      <%= render 'devise/registrations/new' %>
    </div>
  </div>
</div>

